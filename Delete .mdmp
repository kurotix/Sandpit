$drive = "E:"
$searchPath = Join-Path -Path $drive -ChildPath "jenkins-*"

Get-ChildItem -Path $searchPath -Recurse -File | Where-Object { $_.Name -like "*.mdmp" } | ForEach-Object {
    Write-Host "Suppression du fichier : $($_.FullName)"
    Remove-Item $_.FullName -Force
}

Write-Host "Suppression des fichiers .mdmp termin√©e."
