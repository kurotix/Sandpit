$drive = "E:"
$searchPath = Join-Path -Path $drive -ChildPath "jenkins-*"

Get-ChildItem -Path $searchPath -Recurse -File | Where-Object { $_.Name -like "*.mdmp" -or $_.Name -like "*.minidump" } | ForEach-Object {
    Write-Host "Suppression du fichier : $($_.FullName)"
    Remove-Item $_.FullName -Force
}

Write-Host "Suppression des fichiers mini-dump termin√©e."
