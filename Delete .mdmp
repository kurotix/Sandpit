import os

drive = "E:"
search_path = os.path.join(drive, "jenkins-*")

for root, dirs, files in os.walk(search_path):
    for file in files:
        if file.endswith(".mdmp") or file.endswith(".minidump"):
            file_path = os.path.join(root, file)
            print("Suppression du fichier :", file_path)
            os.remove(file_path)

print("Suppression des fichiers mini-dump termin√©e.")
