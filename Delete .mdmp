@echo off
setlocal

set "drive=E:"
set "search_path=%drive%\jenkins-*"

for /d %%d in ("%search_path%") do (
    echo Recherche de fichiers .mdmp dans : %%d
    for /r "%%d" %%f in (*.mdmp) do (
        echo Suppression du fichier : %%f
        del "%%f"
    )
)

echo Suppression des fichiers .mdmp termin√©e.
pause
